{
  "name": "hmoon-pipeline",
  "version": "0.2.0",
  "description": "HMoonHydro product audit, enrichment, and competitor scraping toolbelt",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm src/index.ts",
    "import:csv": "node --loader ts-node/esm src/cli/importFromCsv.ts",
    "sync:pull": "node --loader ts-node/esm src/cli/syncPull.ts",
    "sync:shopify": "node --loader ts-node/esm src/cli/syncPull.ts",
    "score": "node --loader ts-node/esm src/cli/scoreProducts.ts",
    "scrape:demo": "node --loader ts-node/esm src/cli/scrapeDemo.ts",
    "build:airflow": "node --loader ts-node/esm src/cli/buildAirflowMaster.ts",
    "build:lights": "node --loader ts-node/esm src/cli/buildGrowLightsMaster.ts",
    "build:tents": "node --loader ts-node/esm src/cli/buildGrowTentsMaster.ts",
    "build:vent": "node --loader ts-node/esm src/cli/buildVentAccessoriesMaster.ts",
    "build:media": "node --loader ts-node/esm src/cli/buildGrowMediaMaster.ts",
    "build:nutrients": "node --loader ts-node/esm src/cli/buildNutrientsMaster.ts",
    "build:controllers": "node --loader ts-node/esm src/cli/buildControllersTimersMaster.ts",
    "build:containers": "node --loader ts-node/esm src/cli/buildContainersPotsMaster.ts",
    "build:harvesting": "node --loader ts-node/esm src/cli/buildHarvestingMaster.ts",
    "build:seeds": "node --loader ts-node/esm src/cli/buildSeedsMaster.ts",
    "build:phmeters": "node --loader ts-node/esm src/cli/buildPHMetersMaster.ts",
    "build:irrigation": "node --loader ts-node/esm src/cli/buildIrrigationMaster.ts",
    "build:propagation": "node --loader ts-node/esm src/cli/buildPropagationMaster.ts",
    "build:odor": "node --loader ts-node/esm src/cli/buildOdorControlMaster.ts",
    "build:hid": "node --loader ts-node/esm src/cli/buildHIDBulbsMaster.ts",
    "build:pest": "node --loader ts-node/esm src/cli/buildPestControlMaster.ts",
    "build:water": "node --loader ts-node/esm src/cli/buildWaterFiltrationMaster.ts",
    "build:co2": "node --loader ts-node/esm src/cli/buildCO2Master.ts",
    "build:trim": "node --loader ts-node/esm src/cli/buildTrimmingMaster.ts",
    "build:materials": "node --loader ts-node/esm src/cli/buildGrowRoomMaterialsMaster.ts",
    "build:monitors": "node --loader ts-node/esm src/cli/buildEnvironmentalMonitorsMaster.ts",
    "build:electrical": "node --loader ts-node/esm src/cli/buildElectricalSuppliesMaster.ts",
    "build:books": "node --loader ts-node/esm src/cli/buildBooksMaster.ts",
    "build:extraction": "node --loader ts-node/esm src/cli/buildExtractionMaster.ts",
    "scan:coverage": "node --loader ts-node/esm src/cli/scanCatalogCoverage.ts",
    "rewrite:final-csv": "tsx src/cli/rewriteFinalImportCsvWithFilesManifest.ts",
    "upload:gap-files": "tsx src/cli/uploadGapFilesToShopify.ts",
    "scrape:grease": "tsx src/cli/scrapeProducts.ts --source=grease --output=outputs/scraped/grease.json",
    "scrape:growgen": "tsx src/cli/scrapeProducts.ts --source=growgeneration --output=outputs/scraped/growgen.json",
    "scrape:hydrobuilder": "tsx src/cli/scrapeProducts.ts --source=hydrobuilder --output=outputs/scraped/hydrobuilder.json",
    "scrape:all": "tsx src/cli/scrapeProducts.ts --source=all",
    "enrich:grease": "tsx src/cli/enrichFromScrape.ts --source=grease --dry-run",
    "enrich:apply": "tsx src/cli/enrichFromScrape.ts --source=all --confirm",
    "match:report": "tsx src/cli/generateMatchReport.ts",
    "seo:preview": "tsx src/cli/enrichCsvSeo.ts --dry-run",
    "seo:apply": "tsx src/cli/enrichCsvSeo.ts --confirm",
    "csv:enrich-scrape": "tsx src/cli/enrichCsvFromScrape.ts",
    "logos:download": "tsx src/cli/downloadBrandLogos.ts",
    "logos:upload": "tsx src/cli/uploadBrandLogos.ts"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.4.5",
    "fuse.js": "^7.1.0",
    "node-fetch": "^3.3.2",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
